<?xml version="1.0" encoding="UTF-8"?>
<eventReceiver name="LogEventReceiver" statistics="disable"
               trace="disable" xmlns="http://wso2.org/carbon/eventreceiver">
<from eventAdapterType="http">
    <property name="transports">all</property>
    <property name="basicAuthEnabled">true</property>
</from>
<mapping customMapping="disable" type="json"/>
<to streamName="org.wso2.appcloud.container.log.stream" version="1.0.0"/>
</eventReceiver>
